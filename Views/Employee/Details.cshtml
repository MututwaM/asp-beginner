@model mvcdemo.Models.Employee

@{
    ViewData["Title"] = "Employee Details";
}

<h1>Employee Details</h1>

<table style="font-family:Arial">
    <tr>
        <td>
            <b>Employee Id : </b>
        </td>
        <td>
            @Model.EmployeeId
        </td>
    </tr>
    <tr>
        <td>
            <b>Employee Name : </b>
        </td>
        <td>
            @Model.Name
        </td>
    </tr>
    <tr>
        <td>
            <b>Employee Gender : </b>
        </td>
        <td>
            @Model.Gender
        </td>
    </tr>
    <tr>
        <td>
            <b>Employee City : </b>
        </td>
        <td>
            @Model.City
        </td>
    </tr>
</table>

